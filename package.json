{
    "name": "zigzags-workshop",
    "version": "0.0.1",
    "description": "Come over for a minute. I've got a deal for you!",
    "productName": "Zigzag's Workshop",
    "author": "michaelprys",
    "type": "module",
    "private": false,
    "scripts": {
        "dev": "quasar dev",
        "type-check": "vue-tsc --noEmit",
        "fix": "eslint -c ./eslint.config.js --fix \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
        "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
        "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
        "test": "echo \"No test specified\" && exit 0",
        "build": "quasar build",
        "postinstall": "quasar prepare"
    },
    "dependencies": {
        "@pinia/colada": "^0.13.7",
        "@quasar/extras": "^1.16.17",
        "@stripe/stripe-js": "^5.10.0",
        "@supabase/supabase-js": "^2.49.1",
        "gsap": "^3.12.7",
        "pinia": "^2.3.1",
        "pinia-plugin-persistedstate": "^4.2.0",
        "quasar": "^2.18.1",
        "vue": "^3.5.13",
        "vue-draggable-plus": "^0.6.0",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@quasar/app-vite": "^2.1.4",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.21.0",
        "eslint-plugin-vue": "^9.33.0",
        "globals": "^15.15.0",
        "prettier": "^3.5.3",
        "typescript": "5.7.3",
        "vite-plugin-checker": "^0.9.0",
        "vue-tsc": "^2.2.8"
    },
    "engines": {
        "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    }
}
