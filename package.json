{
    "name": "zigzags-workshop",
    "version": "0.0.1",
    "description": "Come over for a minute. I've got a deal for you!",
    "productName": "Zigzag's Workshop",
    "author": "michaelprys",
    "type": "module",
    "private": false,
    "scripts": {
        "dev": "quasar dev",
        "type-check": "vue-tsc --noEmit",
        "fix": "eslint -c ./eslint.config.js --fix \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
        "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
        "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
        "test": "echo \"No test specified\" && exit 0",
        "build": "quasar build",
        "postinstall": "quasar prepare"
    },
    "dependencies": {
        "@pinia/colada": "^0.16.1",
        "@quasar/extras": "^1.17.0",
        "@stripe/stripe-js": "^7.3.0",
        "@supabase/supabase-js": "^2.49.8",
        "gsap": "^3.13.0",
        "pinia": "^3.0.2",
        "pinia-plugin-persistedstate": "^4.3.0",
        "quasar": "^2.18.1",
        "sass": "^1.89.0",
        "vue": "^3.5.15",
        "vue-draggable-plus": "^0.6.0",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.27.0",
        "@quasar/app-vite": "^2.2.0",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.0",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.27.0",
        "eslint-plugin-vue": "^10.1.0",
        "globals": "^16.2.0",
        "prettier": "^3.5.3",
        "typescript": "5.8.3",
        "vite-plugin-checker": "^0.9.3",
        "vue-tsc": "^2.2.10"
    },
    "engines": {
        "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    }
}
